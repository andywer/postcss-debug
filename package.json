{
  "name": "postcss-debug",
  "version": "0.1.1",
  "description": "PostCSS debugger",
  "keywords": [ "postcss", "debug", "benchmark", "inspect" ],
  "scripts": {
    "build": "npm run build:transpile && npm run build:webdebugger",
    "build:transpile": "babel src --out-dir lib",
    "build:webdebugger": "cd webdebugger && npm run build",
    "precommit": "npm run build:transpile",
    "prepush": "npm test",
    "test": "standard src/"
  },
  "bin": "./lib/cli.js",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "author": "Andy Wermke <andy@dev.next-step-software.com>",
  "license": "MIT",
  "homepage": "https://github.com/andywer/postcss-debug",
  "bugs": {
    "url": "https://github.com/andywer/postcss-debug/issues"
  },
  "dependencies": {
    "midas": "^1.2.1",
    "opener": "^1.4.1",
    "postcss": "^5.0.19",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-register": "^6.7.2",
    "husky": "^0.11.4",
    "standard": "^6.0.8"
  }
}
